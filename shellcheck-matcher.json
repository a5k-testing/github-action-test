{
  "problemMatcher": [
    {
      "owner": "shellcheck-matcher",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(?:\\033[^m]+m)?In\\s(.+?)\\sline\\s(\\d+):(?:\\033[^m]+m)?$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": ""
        },
        {
          "regexp": "\\033\\[32m[^-]-+\\^?\\s(SC\\d{4,}):\\s(.+?)(\\033[^m]+m)?$",
          "code": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]

  "problemMatcher": [
    {
      "owner": "shellcheck-matcher",
      "pattern": [
        {
          "regexp": "^(?:\\033[^m]+m)?In\\s(.+?)\\sline\\s(\\d+):(?:\\033[^m]+m)?$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": ""
        },
        {
          "regexp": "--+\\^?\\s(SC\\d{4,}):\\s(.+?)(\\033[^m]+m)?$",
          "code": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
