{
  "problemMatcher": [
    {
      "owner": "shellcheck-matcher-colors",
      "severity": "notice",
      "pattern": [
        {
          "regexp": "^\\033[^m]+mIn (.+?) line (\\d+):\\033[^m]+m$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": ""
        },
        {
          "regexp": "(SC\\d{4}): (.+)\\033[^m]+m$",
          "code": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
