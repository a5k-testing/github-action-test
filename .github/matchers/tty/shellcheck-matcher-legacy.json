{
  "problemMatcher": [
    {
      "owner": "shellcheck-matcher-legacy",
      "pattern": [
        {
          "regexp": "^\\033[^I]+In (.+) line (\\d+):",
          "file": 1,
          "line": 2
        },
        {
          "regexp": "^."
        },
        {
          "regexp": "(SC\\d{4}): (.+)\\033",
          "code": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
