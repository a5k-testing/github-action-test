{
  "problemMatcher": [
    {
      "owner": "shellcheck-matcher",
      "severity": "info",
      "pattern": [
        {
          "regexp": "^\\033[^m]+mIn\\s(.+)\\sline\\s(\\d+):\\033[^m]+m$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": ""
        },
        {
          "regexp": "^\\033\\[32m[^-]*-+\\^?\\s(SC\\d{4}):\\s(.+)\\033[^m]+m$",
          "code": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
